<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section 
      name="darkFlow"
      type="SimpleConfig.Section, SimpleConfig"/>
  </configSections>
  <darkFlow>
    <dispatcher 
      maxConcurrency="10"/>
    <executors>
      <executor
          name="pipeline"
          priority="3"
          maxConcurrency="4"/>
    </executors>

    <routes>
      <matcher
          type="namespace"
          routeTo="pipeline"
          mask="Codestellation.*" />

      <matcher
          type="attribute"
          routeTo="pipeline"
          assembly="Codestellation.DarkFlow"
          attributes="PutItHere" />
    </routes>

    <persistence 
      database="esent">
      <matchers>
        <matcher
            type="namespace"
            mask="Codestellation.*" />

        <matcher
            type="attribute"
            assembly="Codestellation.DarkFlow"
            attributes="PutItHere" />
      </matchers>
    </persistence>
  </darkFlow>

  <system.serviceModel>
    <client>
      <endpoint
        name="Client"
        address="net.tcp://localhost:8095"
        contract="Codestellation.DarkFlow.Tests.IClient"
        binding="netTcpBinding" />
    </client>
    
    <services>
      <service name="Codestellation.DarkFlow.Tests.Service" >
        <endpoint 
          name="Codestellation.DarkFlow.Tests.Service"
          address="net.tcp://localhost:8095"
          contract="Codestellation.DarkFlow.Tests.IAsyncService"
          binding="netTcpBinding"
          bindingName="KharonBinding" 
          endpointConfiguration=""/>
      </service>
      
    </services>
    
    <bindings>
      <netTcpBinding>
        <binding 
          name="KharonBinding">
          <security mode="None">
            
          <transport clientCredentialType="None"/>
            <message clientCredentialType="None"/>
    
          </security>
        </binding>
        
      </netTcpBinding>
    </bindings>

    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>
</configuration>